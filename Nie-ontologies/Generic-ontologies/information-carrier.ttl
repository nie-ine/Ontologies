@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix cidoc: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix frbroo: <http://iflastandards.info/ns/fr/frbr/frbroo/>.
@prefix physical: <http://www.e-editiones.ch/ontology/physical-entity#>.
@prefix event: <http://www.e-editiones.ch/ontology/event#>.
@prefix human: <http://www.e-editiones.ch/ontology/human#>.
@prefix creating: <http://www.e-editiones.ch/ontology/creating#>.
@prefix concept: <http://www.e-editiones.ch/ontology/concept#>.
@prefix math: <http://www.e-editiones.ch/ontology/mathematics#>.
@prefix document: <http://www.e-editiones.ch/ontology/document#>.
@prefix text: <http://www.e-editiones.ch/ontology/text#>.
@prefix text-expression: <http://www.e-editiones.ch/ontology/text-expression#>.
@prefix device: <http://www.e-editiones.ch/ontology/device#>.
@prefix image: <http://www.e-editiones.ch/ontology/image#>.
@prefix letter-corresponding: <http://www.e-editiones.ch/ontology/letter-corresponding#>.
@prefix ling: <http://www.e-editiones.ch/ontology/linguistics#>.

@base <http://www.e-editiones.ch/ontology/information-carrier>.
@prefix infocar: <http://www.e-editiones.ch/ontology/information-carrier#>.

<http://www.e-editiones.ch/ontology/information-carrier>
	a owl:Ontology;
	dct:license <http://creativecommons.org/licenses/by/3.0/>;
	dct:title "An ontology about information carrier"@en;
	dct:description """Formal description of information carrier in broadest sense, general subclasses, related classes, and properties."""@en;
	dct:creator "Hans Cools, MD, knowledge engineer, ontologist, software developer, Basel University, Switzerland"@en;
	dct:publisher "Universities of Basel, Bern, and Zürich, Switzerland"@en.

#
# CLASSES
#

infocar:Carrier
 	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
 	rdfs:label "information carrier"@en,"Informationsträger"@de, "support d'information"@fr;
 	rdfs:comment """Carrier of information created by a person."""@en;
	skos:note """Includes both F4_Manifestation_Singleton and F5_Item as declared in FRBROO."""@en;
	rdfs:subClassOf creating:PhysicalCreation, cidoc:E84_Information_Carrier.

infocar:Fragment
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
 	rdfs:label "information carrier fragment"@en,"Informationsträgerfragment"@de; #, "support d'information"@fr;
 	rdfs:comment """Fragment of information on a carrier."""@en;
 	skos:note """Fragments can possibly be reassembled."""@en;
	rdfs:subClassOf creating:PhysicalCreation.

infocar:Remnant
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
 	rdfs:label "information carrier remnant"@en,"Informationsträgerüberrest"@de; #, "support d'information"@fr;
 	rdfs:comment """Remnant of information on a carrier."""@en;
	rdfs:subClassOf creating:PhysicalCreation.

infocar:Set
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "information carrier set"@en, "Informationsträger-Reihe"@de, "ensemble des supports d'information"@fr;
	rdfs:comment """Information carriers belonging together, e.g. a partitioned work, a nachlass."""@en;
	rdfs:subClassOf creating:CreationSet, physical:Matter.
	# infocar:hasCarrierDescription

infocar:Collection
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "information carrier collection"@en, "Informationsträger-Kollektion"@de, "collection des supports d'information"@fr;
	rdfs:comment """Collection of information carriers, e.g. an archive."""@en;
	rdfs:subClassOf creating:Collection, creating:PhysicalCreation.

infocar:Draft
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "information draft carrier"@en, "Informationsentwurfträger"@de;
	rdfs:comment """Carrier with a draft of information."""@en;
	rdfs:subClassOf infocar:Carrier, creating:UnfinishedCreation.

infocar:Copy
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "information copy carrier"@en, "Informationskopieträger"@de;
	rdfs:comment """Carrier with a copy of information."""@en;
	rdfs:subClassOf infocar:Carrier, creating:Copy.

infocar:Photocopy
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "photocopy"@en, "Fotokopie"@de;
	rdfs:comment """Photographic information copy carrier."""@en;
	rdfs:subClassOf infocar:Copy. # image:PhotographCarrier

infocar:Compilation
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "compilation"@en, "Zusammenstellung"@de;
	rdfs:comment """Carrier with an assemblage of previously separate information."""@en;
	rdfs:subClassOf infocar:Carrier.

infocar:Leaf
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "leaf"@en, "Blatt"@de;
	rdfs:comment """Sheet of a writable substance, e.g. paper, parchment, treated plant leaf."""@en;
	rdfs:subClassOf infocar:Carrier.

# folio, recto of, verso of, number
# fold, Falte, cf. Gedicht Heinrich Heine

infocar:Page
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "page"@en, "Seite"@de;
	rdfs:comment """One of the 2 surfaces of a leaf, e.g. a manuscript, or of a leaf as part of a bundled set of leafs, e.g. a book."""@en;
	rdfs:subClassOf infocar:Carrier.

infocar:Book
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "book"@en, "Buch"@de;
	rdfs:comment """Set of leafs, e.g. sheets of paper or parchment, binded together along one side, with a front and back cover."""@en;
	rdfs:subClassOf infocar:Carrier.

infocar:Binding
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "binding"@en, "Einband"@de;
	rdfs:comment """Material holding the leafs and covers of a book together."""@en;
	rdfs:subClassOf infocar:Carrier.

infocar:PartitionedWork
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "partitioned work"@en, "partitioniertes Werk"@de;
	rdfs:comment """Set of volumes carrying a common expression, e.g. a dictionary."""@en;
	rdfs:subClassOf infocar:Set.

infocar:Volume
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "volume"@en, "Band"@de;
	rdfs:comment """One of a set of books, carrying a part of the common expression of the set."""@en;
	rdfs:subClassOf infocar:Book.

infocar:VolumeOrdinalNumber
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "volume ordinal number"@en, "Bandordinalzahl"@de;
	rdfs:comment """Ordinal number of a volume."""@en;
	rdfs:subClassOf math:OrdinalNumber. # identifier

infocar:Nachlass
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "nachlass"@en, "Nachlass"@de;
	rdfs:comment """Set of information carriers as materialized expressions of a person as existing after the person's death, and unpublished during life."""@en;
	rdfs:subClassOf infocar:Set, cidoc:E78_Collection.

infocar:NachlassElement
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "nachlass element"@en, "Nachlass-Element"@de;
	rdfs:comment """Element of a nachlass."""@en;
	rdfs:subClassOf infocar:Carrier.

infocar:Archive # Library
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "archive"@en, "Archiv"@de, "archive"@fr, "archivio"@it;
	rdfs:comment """Set of information carriers ordered along a method."""@en;
	rdfs:subClassOf infocar:Collection, cidoc:E78_Collection.

infocar:ArchiveElement
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "archive element"@en, "Archiv-Element"@de;
	rdfs:comment """Element of an archive, which can be a single information carrier or a group."""@en;
	rdfs:subClassOf infocar:Carrier.

infocar:WritingUtensil
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "writing utensil"@en, "Schreibzeug"@de;
	rdfs:comment """Device to write with."""@en;
	rdfs:subClassOf device:Device.

infocar:Pencil
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "pencil"@en, "Bleistift"@de;
	rdfs:comment """Elongated cylindrical(-like) utensil for writing or drawing, containing a thin rod of graphite."""@en;
	rdfs:subClassOf infocar:WritingUtensil.

infocar:ColouredPencil
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "coloured pencil"@en, "Farbstift"@de, "crayon de couleur"@en, "matita colorata"@de;
	rdfs:comment """Elongated cylindrical(-like) utensil for writing or drawing, containing a pigmented wax- or oil-based core."""@en;
	rdfs:subClassOf infocar:WritingUtensil.

infocar:Pen
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "pen"@en, "Tintenstift"@de;
	rdfs:comment """Elongated cylindrical(-like) hollow object containing ink for writing or drawing."""@en;
	rdfs:subClassOf infocar:WritingUtensil.

infocar:FountainPen
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "fountain pen"@en, "Füller"@de;
	rdfs:comment """Pen with refillable reservoir for fluid ink for feeding a metal nib."""@en;
	rdfs:subClassOf infocar:Pen.

infocar:Typewriter
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "typewriter"@en, "Schreibmaschine"@de;
	rdfs:comment """Writing device with moving parts for letter printing."""@en; # mechanical or electronic 
	rdfs:subClassOf infocar:WritingUtensil.

infocar:Manuscript
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "manuscript"@en, "Manuskript"@de;
	rdfs:comment """Carrier with a handwritten text."""@en;
	rdfs:subClassOf infocar:Carrier, frbroo:F4_Manifestation_Singleton.

infocar:ManuscriptIdentifier
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "manuscript identifier"@en, "Manuskript-Identifikator"@de;
	rdfs:comment """Identifier of a manuscript, comprising its location name, preserving institution name, repository name, collection name, number, name, and alternative identifier."""@en;
	rdfs:seeAlso <https://www.tei-c.org/release/doc/tei-p5-doc/en/html/ref-msIdentifier.html>;
	rdfs:subClassOf concept:Identifier.

infocar:ManuscriptCollection
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "manuscript collection"@en, "Manuskriptkollektion"@de, "collection des manuscrits"@fr;
	rdfs:comment """Collection of manuscripts."""@en;
	rdfs:subClassOf infocar:Collection.

infocar:Codex
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "codex"@en, "Kodex"@de;
	rdfs:comment """Book as manuscript."""@en;
	rdfs:subClassOf infocar:Book, infocar:Manuscript.

infocar:CodicologicalPart
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "codicological part"@en, "codicologisches Teil"@de;
	rdfs:comment """Part taken from a codex, together with other ones forming a composite manuscript."""@en;
	rdfs:subClassOf infocar:Manuscript.

infocar:CompositeManuscript
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "composite manuscript"@en, "zusammengesetztes Manuskript"@de;
#	ling:hasSynonym "composite codex"@en, "zusammengesetzter Kodex"@de;
	rdfs:comment """Codex reassembled from codicological parts."""@en;
	rdfs:subClassOf infocar:Codex.

infocar:CompiledManuscript
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "compiled manuscript"@en, "zusammengestelltes Manuskript"@de;
	rdfs:comment """Manuscript as a compilation."""@en;
	rdfs:subClassOf infocar:Manuscript, infocar:Compilation.

infocar:PalmLeafManuscript
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "palm-leaf manuscript"@en, "Palmblattmanuskript"@de;
	rdfs:comment """Palm leaf manuscript."""@en;
	skos:note """Dried and smoke treated palm leaves were used as writing carrier in the Indian subcontinent and Southeast since at least the 5th century BC, possibly much earlier."""@en;
	rdfs:subClassOf infocar:Leaf, infocar:Manuscript.

infocar:NoteCarrier
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "note carrier"@en,"Notizträger"@de;
	rdfs:comment """Carrier whereon notes are written, e.g. a first registration of thoughts."""@en;
	rdfs:subClassOf infocar:Carrier, frbroo:F4_Manifestation_Singleton.

infocar:Notebook
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "notebook"@en,"Notizbuch"@de;
	rdfs:comment """Book wherein notes are written."""@en;
	rdfs:subClassOf infocar:Book, infocar:NoteCarrier.

infocar:Typescript
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "typescript"@en, "Typoskript"@de;
	rdfs:comment """Carrier with a typewritten text."""@en;
	rdfs:subClassOf infocar:Carrier, frbroo:F4_Manifestation_Singleton.

infocar:TextCopy
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "text copy"@en, "Textkopie"@de;
	rdfs:comment """Carrier with a copy of a text."""@en;
	rdfs:subClassOf infocar:Copy.

infocar:Transcript
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "transcript"@en, "Transkript"@de;
#	ling:hasSynonym "Abschrift"@de;
	rdfs:comment """Text copy created by character, by any writing method, e.g. handwriting, typewriting, digitizing."""@en;
	rdfs:subClassOf infocar:TextCopy, frbroo:F4_Manifestation_Singleton.

infocar:ManualTranscript
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "manual transcript"@en, "manuelles Transkript"@de;
	rdfs:comment """Handwritten transcript."""@en;
	rdfs:subClassOf infocar:Transcript, infocar:Manuscript.

infocar:Printing
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "printing"@en, "Drucken"@de;
	rdfs:comment """Action of putting an expression on a carrier, e.g. with ink on paper."""@en;
	rdfs:subClassOf human:Procedure, frbroo:F32_Carrier_Production_Event.

infocar:AgentPrinter
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "agent printer"@en, "Agent-Drucker"@de;
	rdfs:comment """Role of a human agent, person or organization, printing an expression."""@en;
	rdfs:subClassOf human:AgentRole.

infocar:PersonPrinter
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "person printer"@en, "Person-Drucker"@de;
	rdfs:comment """Role of a person printing an expression."""@en;
	rdfs:subClassOf creating:PersonCreator, infocar:AgentPrinter.

infocar:Print
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "print"@en, "Druck"@de;
	rdfs:comment """Something printed on a carrier."""@en;
	rdfs:subClassOf infocar:Carrier.

infocar:PrintIdentifier
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "print identifier"@en, "Druck-Identifikator"@de;
	rdfs:comment """Identifier of a print, comprising its location, preserving organization, repository, collection, number, name, and alternative."""@en;
	rdfs:subClassOf concept:Identifier.

infocar:Incunabulum
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "incunabulum"@en, "Inkunabel"@de;
	rdfs:comment """Print created In Europe prior to 1501, during the earliest years of printing."""@en;
	rdfs:subClassOf infocar:Print.

infocar:PrintRun
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "print run"@en, "Druckauflage"@de, "Tirage"@fr, "Tiratura"@it;
	rdfs:comment """Set of prints of the same expression, created at one time."""@en;
	skos:note """When applicable, all the prints have the same ISBN, but a different bar code."""@en;
	rdfs:subClassOf infocar:Set.

infocar:FirstPrint
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "first print"@en, "Erstdruck"@de;
	rdfs:comment """Earliest print of an expression."""@en;
	rdfs:subClassOf infocar:Print.

infocar:CompiledPrint
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "compiled print"@en, "zusammengestellter Druck"@de;
	rdfs:comment """Print as a compilation."""@en;
	rdfs:subClassOf infocar:Print, infocar:Compilation.

infocar:Diary
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "diary"@en,"Tagebuch"@de;
	rdfs:comment """Information carrier with periodically, usually daily, described personal experiences and observations."""@en;
	rdfs:subClassOf infocar:Carrier.

infocar:PersonSender
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "person sender"@en, "Person-Sender"@de;
	rdfs:comment """Role of a person sending information on a carrier."""@en;
	rdfs:subClassOf human:PersonSender.

infocar:PersonReceiver
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "person receiver"@en, "Person-Empfänger"@de;
	rdfs:comment """Role of a person receiving information on carrier."""@en;
	rdfs:subClassOf human:PersonReceiver.

infocar:Postcard
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "postcard"@en, "Postkarte"@de, "carte postale"@fr, "cartolina"@it;
	rdfs:comment """Card with one side, usually with a printed structure, used for writing a message, an address and applying a postage stamp, to be sent through a postal system."""@en;
	rdfs:subClassOf infocar:Carrier, frbroo:F4_Manifestation_Singleton.

infocar:Convolute
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "information carrier convolute"@en, "Informationsträger-Konvolut"@de;
	rdfs:comment """Set of information carriers, e.g. handwritten drafts, manuscripts, typoscripts or prints, with a commonality, e.g. being of the same author."""@en;
	rdfs:subClassOf infocar:Set.

infocar:ConvoluteElement
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "convolute element"@en, "Konvolut-Element"@de;
	rdfs:comment """Element of an information carrier convolute."""@en;
	rdfs:subClassOf infocar:Carrier.

infocar:ElectronicCarrier #° data or information storage device
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "electronic information carrier"@en,"elektronischer Informationsträger"@de;
	rdfs:comment """Electronic machine carrying electronic information."""@en;
	rdfs:subClassOf device:ElectronicMachine, infocar:Carrier.

# infocar:Scan

infocar:ElectronicBook
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "electronic book"@en,"elektronisches Buch"@de;
	rdfs:comment """Electronic information carrier with a text structure as in a book."""@en;
	rdfs:subClassOf infocar:ElectronicCarrier.

infocar:Webpage
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "webpage"@en, "Webseite"@de;
	rdfs:comment """Electronic carrier page on the World Wide Web."""@en;
	rdfs:subClassOf infocar:ElectronicCarrier.

infocar:Website
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "website"@en, "Website"@de;
	rdfs:comment """Set of interconnected webpages, under a single domain name, usually including a homepage, generally located on the same server, and prepared and maintained as information by a person, group, or organization."""@en;
	rdfs:subClassOf infocar:ElectronicCarrier, infocar:Set.

infocar:Wiki
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "wiki"@en, "Wiki"@de;
	rdfs:comment """Knowledge base website on which users collaboratively modify and structure content directly from a web browser; it is ran by wiki software."""@en;
	rdfs:seeAlso <https://en.wikipedia.org/wiki/Wiki>;
	rdfs:subClassOf infocar:Website.

infocar:DokuWiki
	rdfs:isDefinedBy <http://www.e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "DokuWiki"@en, "DokuWiki"@de;
	rdfs:comment """Wiki ran by DokuWiki software."""@en;
