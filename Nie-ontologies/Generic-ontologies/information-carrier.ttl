@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix cidoc: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix frbroo: <http://iflastandards.info/ns/fr/frbr/frbroo/>.
@prefix physical-entity: <http://e-editiones.ch/ontology/physical-entity#>.
@prefix event: <http://e-editiones.ch/ontology/event#>.
@prefix human: <http://e-editiones.ch/ontology/human#>.
@prefix creating: <http://e-editiones.ch/ontology/creating#>.
@prefix concept: <http://e-editiones.ch/ontology/concept#>.
@prefix mathematics: <http://e-editiones.ch/ontology/mathematics#>.
@prefix document: <http://e-editiones.ch/ontology/document#>.
@prefix text: <http://e-editiones.ch/ontology/text#>.
@prefix text-expression: <http://e-editiones.ch/ontology/text-expression#>.
@prefix device: <http://e-editiones.ch/ontology/device#>.
@prefix image: <http://e-editiones.ch/ontology/image#>.
@prefix letter-corresponding: <http://e-editiones.ch/ontology/letter-corresponding#>.
@prefix linguistics: <http://e-editiones.ch/ontology/linguistics#>.

@base <http://e-editiones.ch/ontology/information-carrier>.
@prefix information-carrier: <http://e-editiones.ch/ontology/information-carrier#>.

<http://e-editiones.ch/ontology/information-carrier>
	a owl:Ontology;
	dct:license <http://creativecommons.org/licenses/by/3.0/>;
	dct:title "An ontology about information carrier"@en;
	dct:description """Formal description of information carrier in broadest sense, general subclasses, related classes, and properties."""@en;
	dct:creator "Hans Cools, MD, knowledge engineer, ontologist, software developer, University of Basel, Switzerland"@en;
	dct:contributor
		"Roberta Padlina, project coordinator and software developer, University of Basel, Switzerland"@en,
		"Reto Baumgartner, tool coordinator and software developer, University of Zürich, Switzerland"@en,
		"Rebekka Plüss, research assistant and software developer, University of Basel, Switzerland"@en;
	dct:publisher "Universities of Basel, Bern, and Zürich, Switzerland"@en;
	owl:versionInfo "2020-06-11"^^xsd:date.

#
# CLASSES
#

information-carrier:Carrier
 	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
 	rdfs:label "information carrier"@en,"Informationsträger"@de, "support d'information"@fr;
 	rdfs:comment """Carrier of information created by a person."""@en;
	skos:note """Includes both F4_Manifestation_Singleton and F5_Item as declared in FRBRoo."""@en;
	rdfs:subClassOf creating:PhysicalCreation, cidoc:E84_Information_Carrier.

information-carrier:Fragment
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
 	rdfs:label "information carrier fragment"@en,"Informationsträgerfragment"@de; #, "support d'information"@fr;
 	rdfs:comment """Fragment of information on a carrier."""@en;
 	skos:note """Fragments can possibly be reassembled."""@en;
	rdfs:subClassOf creating:PhysicalCreation.

information-carrier:Remnant
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
 	rdfs:label "information carrier remnant"@en,"Informationsträgerüberrest"@de; #, "support d'information"@fr;
 	rdfs:comment """Remnant of information on a carrier."""@en;
	rdfs:subClassOf creating:PhysicalCreation.

information-carrier:Set
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "information carrier set"@en, "Informationsträger-Reihe"@de, "ensemble des supports d'information"@fr;
	rdfs:comment """Information carriers belonging together, e.g. a partitioned work, a nachlass."""@en;
	rdfs:subClassOf creating:CreationSet, physical-entity:Matter.
	# information-carrier:hasCarrierDescription

information-carrier:Collection
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "information carrier collection"@en, "Informationsträger-Sammlung"@de, "collection des supports d'information"@fr;
	rdfs:comment """Collection of information carriers, e.g. an archive."""@en;
	rdfs:subClassOf creating:Collection, creating:PhysicalCreation.

information-carrier:Draft
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "information draft carrier"@en, "Informationsentwurfträger"@de;
	rdfs:comment """Carrier with a draft of information."""@en;
	rdfs:subClassOf information-carrier:Carrier, creating:UnfinishedCreation.

information-carrier:Copy
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "information copy carrier"@en, "Informationskopieträger"@de;
	rdfs:comment """Carrier with a copy of information."""@en;
	rdfs:subClassOf information-carrier:Carrier, creating:Copy.

information-carrier:Photocopy
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "photocopy"@en, "Fotokopie"@de;
	rdfs:comment """Photographic information copy carrier."""@en;
	rdfs:subClassOf information-carrier:Copy. # image:PhotographCarrier

information-carrier:Compilation
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "compilation"@en, "Zusammenstellung"@de;
	rdfs:comment """Carrier with an assemblage of previously separate information."""@en;
	rdfs:subClassOf information-carrier:Carrier.

information-carrier:Leaf
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "leaf"@en, "Blatt"@de;
	rdfs:comment """Sheet of a writable substance, e.g. paper, parchment, treated plant leaf."""@en;
	rdfs:subClassOf information-carrier:Carrier.

# folio, recto of, verso of, number
# fold, Falte, cf. Gedicht Heinrich Heine

information-carrier:Page
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "page"@en, "Seite"@de;
	rdfs:comment """One of the 2 surfaces of a leaf, e.g. a manuscript, or of a leaf as part of a bundled set of leafs, e.g. a book."""@en;
	rdfs:subClassOf information-carrier:Carrier.

information-carrier:Book
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "book"@en, "Buch"@de;
	rdfs:comment """Set of leafs, e.g. sheets of paper or parchment, binded together along one side, with a front and back cover."""@en;
	rdfs:subClassOf information-carrier:Carrier.

information-carrier:Binding
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "binding"@en, "Einband"@de;
	rdfs:comment """Material holding the leafs and covers of a book together."""@en;
	rdfs:subClassOf information-carrier:Carrier.

information-carrier:PartitionedWork
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "partitioned work"@en, "partitioniertes Werk"@de;
	rdfs:comment """Set of volumes carrying a common expression, e.g. a dictionary."""@en;
	rdfs:subClassOf information-carrier:Set.

information-carrier:Volume
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "volume"@en, "Band"@de;
	rdfs:comment """One of a set of books, carrying a part of the common expression of the set."""@en;
	rdfs:subClassOf information-carrier:Book.

information-carrier:VolumeOrdinalNumber
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "volume ordinal number"@en, "Bandordinalzahl"@de;
	rdfs:comment """Ordinal number of a volume."""@en;
	rdfs:subClassOf mathematics:OrdinalNumber. # identifier

information-carrier:Nachlass
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "nachlass"@en, "Nachlass"@de;
	rdfs:comment """Set of information carriers as materialized expressions of a person as existing after the person's death, and unpublished during life."""@en;
	rdfs:subClassOf information-carrier:Set, cidoc:E78_Collection.

information-carrier:NachlassElement
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "nachlass element"@en, "Nachlasselement"@de;
	rdfs:comment """Element of a nachlass."""@en;
	rdfs:subClassOf information-carrier:Carrier.

information-carrier:Archive # Library
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "archive"@en, "Archiv"@de, "archive"@fr, "archivio"@it;
	rdfs:comment """Set of information carriers ordered along a method."""@en;
	rdfs:subClassOf information-carrier:Collection, cidoc:E78_Collection.

information-carrier:ArchiveElement
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "archive element"@en, "Archivelement"@de;
	rdfs:comment """Element of an archive, which can be a single information carrier or a group."""@en;
	rdfs:subClassOf information-carrier:Carrier.

information-carrier:WritingUtensil
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "writing utensil"@en, "Schreibutensilie"@de;
	rdfs:comment """Device to write with."""@en;
	rdfs:subClassOf device:Device.

information-carrier:Pencil
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "pencil"@en, "Bleistift"@de;
	rdfs:comment """Elongated cylindrical(-like) utensil for writing or drawing, containing a thin rod of graphite."""@en;
	rdfs:subClassOf information-carrier:WritingUtensil.

information-carrier:ColouredPencil
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "coloured pencil"@en, "Farbstift"@de, "crayon de couleur"@en, "matita colorata"@de;
	rdfs:comment """Elongated cylindrical(-like) utensil for writing or drawing, containing a pigmented wax- or oil-based core."""@en;
	rdfs:subClassOf information-carrier:WritingUtensil.

information-carrier:Pen
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "pen"@en, "Stift"@de;
	rdfs:comment """Elongated cylindrical(-like) hollow object containing ink for writing or drawing."""@en;
	rdfs:subClassOf information-carrier:WritingUtensil.

information-carrier:FountainPen
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "fountain pen"@en, "Füllfederhalter"@de;
	rdfs:comment """Pen with refillable reservoir for fluid ink for feeding a metal nib."""@en;
	rdfs:subClassOf information-carrier:Pen.

information-carrier:Typewriter
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "typewriter"@en, "Schreibmaschine"@de;
	rdfs:comment """Writing device with moving parts for letter printing."""@en; # mechanical or electronic 
	rdfs:subClassOf information-carrier:WritingUtensil.

information-carrier:Manuscript
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "manuscript"@en, "Manuskript"@de;
	rdfs:comment """Carrier with a handwritten text."""@en;
	rdfs:subClassOf information-carrier:Carrier, frbroo:F4_Manifestation_Singleton.

information-carrier:ManuscriptIdentifier
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "manuscript identifier"@en, "Manuskriptidentifikator"@de;
	rdfs:comment """Identifier of a manuscript, comprising its location name, preserving institution name, repository name, collection name, number, name, and alternative identifier."""@en;
	rdfs:seeAlso <https://www.tei-c.org/release/doc/tei-p5-doc/en/html/ref-msIdentifier.html>;
	rdfs:subClassOf concept:Identifier.

information-carrier:ManuscriptCollection
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "manuscript collection"@en, "Manuskriptsammlung"@de, "collection des manuscrits"@fr;
	rdfs:comment """Collection of manuscripts."""@en;
	rdfs:subClassOf information-carrier:Collection.

information-carrier:Codex
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "codex"@en, "Kodex"@de;
	rdfs:comment """Book as manuscript."""@en;
	rdfs:subClassOf information-carrier:Book, information-carrier:Manuscript.

information-carrier:CodicologicalPart
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "codicological part"@en, "kodikologischer Teil"@de;
	rdfs:comment """Part taken from a codex, together with other ones forming a composite manuscript."""@en;
	rdfs:subClassOf information-carrier:Manuscript.

information-carrier:CompositeManuscript
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "composite manuscript"@en, "zusammengesetztes Manuskript"@de;
#	linguistics:hasSynonym "composite codex"@en, "zusammengesetzter Kodex"@de;
	rdfs:comment """Codex reassembled from codicological parts."""@en;
	rdfs:subClassOf information-carrier:Codex.

information-carrier:CompiledManuscript
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "compiled manuscript"@en, "zusammengestelltes Manuskript"@de;
	rdfs:comment """Manuscript as a compilation."""@en;
	rdfs:subClassOf information-carrier:Manuscript, information-carrier:Compilation.

information-carrier:PalmLeafManuscript
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "palm-leaf manuscript"@en, "Palmblattmanuskript"@de;
	rdfs:comment """Palm leaf manuscript."""@en;
	skos:note """Dried and smoke treated palm leaves were used as writing carrier in the Indian subcontinent and Southeast since at least the 5th century BC, possibly much earlier."""@en;
	rdfs:subClassOf information-carrier:Leaf, information-carrier:Manuscript.

information-carrier:NoteCarrier
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "note carrier"@en,"Notizträger"@de;
	rdfs:comment """Carrier whereon notes are written, e.g. a first registration of thoughts."""@en;
	rdfs:subClassOf information-carrier:Carrier, frbroo:F4_Manifestation_Singleton.

information-carrier:Notebook
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "notebook"@en,"Notizbuch"@de;
	rdfs:comment """Book wherein notes are written."""@en;
	rdfs:subClassOf information-carrier:Book, information-carrier:NoteCarrier.

information-carrier:Typescript
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "typescript"@en, "Typoskript"@de;
	rdfs:comment """Carrier with a typewritten text."""@en;
	rdfs:subClassOf information-carrier:Carrier, frbroo:F4_Manifestation_Singleton.

information-carrier:TextCopy
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "text copy"@en, "Textkopie"@de;
	rdfs:comment """Carrier with a copy of a text."""@en;
	rdfs:subClassOf information-carrier:Copy.

information-carrier:Transcript
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "transcript"@en, "Transkript"@de;
#	linguistics:hasSynonym "Abschrift"@de;
	rdfs:comment """Text copy created by character, by any writing method, e.g. handwriting, typewriting, digitizing."""@en;
	rdfs:subClassOf information-carrier:TextCopy, frbroo:F4_Manifestation_Singleton.

information-carrier:ManualTranscript
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "manual transcript"@en, "manuelles Transkript"@de;
	rdfs:comment """Handwritten transcript."""@en;
	rdfs:subClassOf information-carrier:Transcript, information-carrier:Manuscript.

information-carrier:Printing
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "printing"@en, "Drucken"@de;
	rdfs:comment """Action of putting an expression on a carrier, e.g. with ink on paper."""@en;
	rdfs:subClassOf human:Procedure, frbroo:F32_Carrier_Production_Event.

information-carrier:AgentPrinter
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "printer role of agent"@en, "Druckerrolle eines Akteurs"@de;
	rdfs:comment """Role of a human agent, person or organization, printing an expression."""@en;
	rdfs:subClassOf human:AgentRole.

information-carrier:PersonPrinter
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "printer role of person"@en, "Druckerrolle einer Person"@de;
	rdfs:comment """Role of a person printing an expression."""@en;
	rdfs:subClassOf creating:PersonCreator, information-carrier:AgentPrinter.

information-carrier:Print
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "print"@en, "Druck"@de;
	rdfs:comment """Something printed on a carrier."""@en;
	rdfs:subClassOf information-carrier:Carrier.

information-carrier:PrintIdentifier
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "print identifier"@en, "Druckidentifikator"@de;
	rdfs:comment """Identifier of a print, comprising its location, preserving organization, repository, collection, number, name, and alternative."""@en;
	rdfs:subClassOf concept:Identifier.

information-carrier:Incunabulum
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "incunabulum"@en, "Inkunabel"@de;
	rdfs:comment """Print created In Europe prior to 1501, during the earliest years of printing."""@en;
	rdfs:subClassOf information-carrier:Print.

information-carrier:PrintRun
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "print run"@en, "Druckauflage"@de, "Tirage"@fr, "Tiratura"@it;
	rdfs:comment """Set of prints of the same expression, created at one time."""@en;
	skos:note """When applicable, all the prints have the same ISBN, but a different bar code."""@en;
	rdfs:subClassOf information-carrier:Set.

information-carrier:FirstPrint
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "first print"@en, "Erstdruck"@de;
	rdfs:comment """Earliest print of an expression."""@en;
	rdfs:subClassOf information-carrier:Print.

information-carrier:CompiledPrint
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "compiled print"@en, "zusammengestellter Druck"@de;
	rdfs:comment """Print as a compilation."""@en;
	rdfs:subClassOf information-carrier:Print, information-carrier:Compilation.

information-carrier:Diary
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "diary"@en,"Tagebuch"@de;
	rdfs:comment """Information carrier with periodically, usually daily, described personal experiences and observations."""@en;
	rdfs:subClassOf information-carrier:Carrier.

information-carrier:PersonSender
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "sender role of person"@en, "Senderrolle einer Person"@de;
	rdfs:comment """Role of a person sending information on a carrier."""@en;
	rdfs:subClassOf human:PersonSender.

information-carrier:PersonReceiver
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "receiver role of person"@en, "Empfängerrolle einer Person"@de;
	rdfs:comment """Role of a person receiving information on carrier."""@en;
	rdfs:subClassOf human:PersonReceiver.

information-carrier:Postcard
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "postcard"@en, "Postkarte"@de, "carte postale"@fr, "cartolina"@it;
	rdfs:comment """Card with one side, usually with a printed structure, used for writing a message, an address and applying a postage stamp, to be sent through a postal system."""@en;
	rdfs:subClassOf information-carrier:Carrier, frbroo:F4_Manifestation_Singleton.

information-carrier:Convolute
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "information carrier convolute"@en, "Informationsträger-Konvolut"@de;
	rdfs:comment """Set of information carriers, e.g. handwritten drafts, manuscripts, typoscripts or prints, with a commonality, e.g. being of the same author."""@en;
	rdfs:subClassOf information-carrier:Set.

information-carrier:ConvoluteElement
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a rdfs:Class;
	rdfs:label "convolute element"@en, "Konvolutelement"@de;
	rdfs:comment """Element of an information carrier convolute."""@en;
	rdfs:subClassOf information-carrier:Carrier.

#
# PROPERTIES
#

information-carrier:carrierHasDescriptionLiteral
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a owl:DatatypeProperty;
	rdfs:label "carrier has description literal"@en, "Träger hat Beschreibungliteral"@de;
	rdfs:comment """Relating an information carrier to a description literal thereof."""@en;
	rdfs:domain information-carrier:Carrier;
	rdfs:range rdfs:Literal;
	rdfs:subPropertyOf text:hasDescriptionLiteral.

information-carrier:isCarrierOfSet
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a owl:ObjectProperty;
	rdfs:label "is carrier of set"@en, "ist Träger einer Reihe"@de;
	rdfs:comment """Relating an information carrier to a set it is part of."""@en;
	rdfs:domain information-carrier:Carrier;
	rdfs:range information-carrier:Set;
	rdfs:subPropertyOf physical-entity:isPartOf.

information-carrier:isCarrierOfCollection
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a owl:ObjectProperty;
	rdfs:label "is carrier of collection"@en, "ist Träger einer Sammlung"@de;
	rdfs:comment """Relating an information carrier to a collection it is part of."""@en;
	rdfs:domain information-carrier:Carrier;
	rdfs:range information-carrier:Collection;
	rdfs:subPropertyOf physical-entity:isPartOf.

information-carrier:hasSender
	rdfs:isDefinedBy <http://e-editiones.ch/ontology/information-carrier#>;
	a owl:ObjectProperty;
	rdfs:label "has sender"@en, "hat Absender"@de;
	rdfs:comment """Relating an information carrier to a human agent - a person or a group - that sent it."""@en;
	rdfs:domain information-carrier:Carrier;
	rdfs:range [rdfs:subClassOf human:Agent, [a owl:Restriction; owl:onProperty human:agentHasRole; owl:someValuesFrom information-carrier:Sender]];
	rdfs:subPropertyOf physical-entity:hasSender.

