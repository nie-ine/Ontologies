@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix cidoc: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix frbroo: <http://iflastandards.info/ns/fr/frbr/frbroo/>.
@prefix event: <http://www.knora.org/ontology/0047/event#>.
@prefix human: <http://www.knora.org/ontology/0048/human#>.
@prefix language: <http://www.knora.org/ontology/004F/language#>.
@prefix concept: <http://www.knora.org/ontology/0044/concept#>.
@prefix document: <http://www.knora.org/ontology/005C/document#>.
@prefix text: <http://www.knora.org/ontology/0059/text#>.
@prefix text-structure: <http://www.knora.org/ontology/005B/text-structure#>.
@prefix infocar: <http://www.knora.org/ontology/004C/information-carrier#>.
@prefix publish: <http://www.knora.org/ontology/0056/publishing#>.
@prefix litera: <http://www.knora.org/ontology/0050/literature#>.
@prefix knora-base: <http://www.knora.org/ontology/knora-base#>.

@base <http://www.knora.org/ontology/005A/text-editing>.
@prefix textedit: <http://www.knora.org/ontology/005A/text-editing#>.

<http://www.knora.org/ontology/005A/text-editing>
	a owl:Ontology;
	dct:title "An ontology about text editing"@en;
	dct:description """Formal description of text editing, general subclasses, related classes, and properties."""@en;
	dct:creator "Hans Cools, MD, knowledge engineer, ontologist, software developer, NIE-INE, Basel University, Switzerland"@en;
	dct:contributor "Roberta Padlina, tool coordinator, NIE-INE, Basel University, Switzerland"@en;
	dct:publisher "Basel University, Switzerland"@en;
#	rdfs:seeAlso <https://digitallatin.github.io/guidelines/LDLT-Guidelines.html>;

	knora-base:attachedToProject <http://rdfh.ch/projects/005A>.

#
# CLASSES
#

textedit:Editor
	a owl:Class;
	rdfs:label "text editor"@en, "Text-Editor"@de;
	rdfs:comment """Role of a person changing a text."""@en;
	rdfs:subClassOf human:PersonCreator.

textedit:Edition
	a owl:Class;
	rdfs:label "text edition"@en, "Text-Edition"@de;
	rdfs:comment """Changed text as a result of editing."""@en;
	rdfs:subClassOf text:Text , [ # human:CreationVersion
		a owl:Restriction; owl:onProperty textedit:hasEditor; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:hasEditorValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

textedit:Redactor
	a owl:Class;
	rdfs:label "redactor"@la, "redactor"@en, "Redactor"@de;
	rdfs:comment """Editor assembling parts of texts to a new coherent text."""@en;
	rdfs:subClassOf textedit:Editor.

textedit:Glossator
	a owl:Class;
	rdfs:label "glossator"@la, "glossator"@en, "Glossator"@de;
	rdfs:comment """Role of a person adding glosses to a text."""@en;
	rdfs:subClassOf textedit:Editor.

textedit:Abbreviator
	a owl:Class;
	rdfs:label "abbreviator"@en, "KurzfasserIn"@de, "abbreviator"@la;
	rdfs:comment """Editor shortening a text."""@en;
	rdfs:subClassOf textedit:Editor.

textedit:Abbreviation # derivative?
	a owl:Class;
	rdfs:label "abbreviation"@en, "Kurzfassung"@de;
	rdfs:comment """Shortened edition."""@en;
	rdfs:subClassOf textedit:Edition.

textedit:Paraphrase # derivative? # scientific?
	a owl:Class;
	rdfs:label "paraphrase"@en, "Paraphrase"@de;
	rdfs:comment """Edition restating a text clarifying its meaning."""@en;
	rdfs:subClassOf textedit:Edition.

textedit:LiteralExposition # derivative? # scientific?
	a owl:Class;
	rdfs:label "literal exposition"@en, "wörtliche Auslegung"@de;
	rdfs:comment """Edition providing background information on word level to a text in clarifying its meaning."""@en;
	rdfs:subClassOf textedit:Edition.

textedit:Transliteration
	a owl:Class;
	rdfs:label "transliteration"@en, "Transliteration"@de, "translittération"@fr, "traslitterazione"@it;
	rdfs:comment """Written text transferred in another alphabet using the closest corresponding symbols, e.g. letters."""@en;
	skos:note """Primarily representing the symbols, not the sounds."""@en;
	rdfs:subClassOf text:WrittenText, textedit:Edition.

textedit:DerivedTextExpression
	a owl:Class;
	rdfs:label "derived text expression"@en, "abgeleitete Text-Expression"@de;
	rdfs:comment """Text expression derived from another text expression with content changed beyond a version."""@en;
	rdfs:subClassOf text:Expression, human:CreationDerivative.

textedit:EditionScientist
	a owl:Class;
	rdfs:label "edition scientist"@en, "EditionswissenschaftlerIn"@de, "éditrice scientifique"@fr;
	rdfs:comment """Role of a person practicing edition science."""@en;
	rdfs:subClassOf human:Scientist.

textedit:ScientificEdition
	a owl:Class;
	rdfs:label "scientific edition"@en, "wissenschaftliche Edition"@de;
	rdfs:comment """Derived text expression edited for scientific purpose."""@en;
	rdfs:subClassOf textedit:DerivedTextExpression , [
		a owl:Restriction; owl:onProperty textedit:hasScientificEditor; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:hasScientificEditorValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

textedit:ConstitutedEdition
	a owl:Class;
	rdfs:label "constituted text edition"@en, "konstituierter Text"@de;
#	ling:hasSynonym "Lesetext"@de; # eclectic edition?
	rdfs:comment """Plain text expression as endresult of scientific editing of another text expression."""@en;
	rdfs:subClassOf textedit:ScientificEdition.

textedit:DiplomaticTranscription
	a owl:Class;
	rdfs:label "diplomatic transcription"@en, "diplomatische Umschrift"@de;
#	ling:hasSynonym "Lesehilfe"@de;
	rdfs:comment """Transcription of a text with diacritical signs indicating visual textual properties to describe the textual state."""@en;
	rdfs:subClassOf textedit:ScientificEdition, [
		a owl:Restriction; owl:onProperty textedit:hasDiacriticalSign; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:hasPageIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:hasPageNumberIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:hasColumnIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:isDiplomaticTranscriptionOfTextOnPage; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:isDiplomaticTranscriptionOfTextOnPageValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

# textedit:Diplomatic-InterpretativeTranscription

textedit:CriticalEditor
	a owl:Class;
	rdfs:label "critical text editor"@en, "kritische Text-Editor"@de;
	rdfs:comment """Role of being a scientist editing a text expression in a critical scientific way."""@en;
	rdfs:subClassOf human:Scientist, human:PersonCreator.

textedit:CriticalEdition
	a owl:Class;
	rdfs:label "critical text edition"@en, "kritische Textedition"@de;
	rdfs:comment """Edition to which a critical editor added evidence and interpretation of a text creation and historical transmission, allowing readers to assess the editor's choice."""@en;
	rdfs:subClassOf textedit:ScientificEdition, [
		a owl:Restriction; owl:onProperty textedit:hasCriticalEditor; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:hasCriticalEditorValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

# textedit:Variorum subClassOf CriticalEdition

textedit:GeneticEdition
	a owl:Class;
	rdfs:label "genetic edition"@en, "genetische Edition"@de, "édition génétique"@fr, "edizione genetica"@it;
	rdfs:comment """Critical edition accounting for the modifications by the author."""@en;
	rdfs:subClassOf textedit:CriticalEdition.

textedit:EditorialStructure
	a owl:Class;
	rdfs:label "editorial text structure"@en, "editorische Textstruktur"@de;
	rdfs:comment """Scientific text structure depending on editorial purpose, e.g. an editing apparatus."""@en;
	rdfs:subClassOf text-structure:ScientificStructure, [
		a owl:Restriction; owl:onProperty textedit:hasSiglum; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:hasSiglumValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

textedit:Apparatus
	a owl:Class;
	rdfs:label "editing apparatus"@en, "Edierungsapparatus"@de, "apparatus editionis"@la;
	rdfs:comment """Scientific compositional structure to analyse text in a scientific edition along certain criteria."""@en;
	rdfs:subClassOf textedit:EditorialStructure, text-structure:CompositionalContentStructure, [
		a owl:Restriction; owl:onProperty textedit:isApparatusOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:isApparatusOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

textedit:ApparatusEntry
	a owl:Class;
	rdfs:label "editing apparatus entry"@en, "Edierungsapparateintrag"@de; #, "entrée d'apparat éditionel"@fr, "iscrizione del apparato"@it;
	rdfs:comment """Entry of an editing apparatus."""@en;
	rdfs:subClassOf textedit:EditorialStructure, text-structure:CompositionalContentStructure, [
		a owl:Restriction; owl:onProperty textedit:isEntryOfApparatus; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:isEntryOfApparatusValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:hasApparatusEnteringDate; owl:minCardinality "0"^^xsd:nonNegativeInteger].

textedit:DoctrinalApparatus
	a owl:Class;
	rdfs:label "doctrinal apparatus"@en, "doktrinal Apparat"@de;
#	ling:hasSynonym textedit:HistoricalApparatus;
	rdfs:comment """Editing apparatus commmenting on the content of a scientific text edition in a historical and doctrinal context."""@en; # info about theory
	rdfs:subClassOf textedit:Apparatus.

textedit:ApparatusFontium
	a owl:Class;
	rdfs:label "apparatus fontium"@en, "Apparatus Fontium"@de, "apparatus fontium"@la;
	rdfs:comment """Editing apparatus containing references to one or more citations in a scientific text edition."""@en;
	rdfs:subClassOf textedit:Apparatus.

textedit:CriticalApparatus
	a owl:Class;
	rdfs:label "critical apparatus"@en, "textkritischer Apparat"@de, "apparat critique"@fr, "apparato critico"@it, "apparatus criticus"@la;
	rdfs:comment """Editing apparatus to a critical text edition providing information of textual versions."""@en;
	rdfs:subClassOf textedit:Apparatus, [
		a owl:Restriction; owl:onProperty textedit:isCriticalApparatusOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:isCriticalApparatusOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

textedit:CriticalApparatusEntry
	a owl:Class;
	rdfs:label "critical apparatus entry"@en, "textkritischer Apparateintrag"@de, "entrée d'apparat critique"@fr, "iscrizione del apparato critico"@it;
	rdfs:comment """Entry of critical apparatus."""@en;
	rdfs:subClassOf textedit:ApparatusEntry, [
		a owl:Restriction; owl:onProperty textedit:isEntryOfCriticalApparatus; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:isEntryOfCriticalApparatusValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

textedit:StructureApparatus
	a owl:Class;
	rdfs:label "text structure apparatus"@en, "Textstruktur-Apparat"@de;
	rdfs:comment """Editing apparatus to a critical text edition providing information of text structure."""@en;
	rdfs:subClassOf textedit:Apparatus.

textedit:StructureApparatusEntry
	a owl:Class;
	rdfs:label "structure apparatus entry"@en, "Textstruktur-Apparateintrag"@de; #, "entrée d'apparat de structure"@fr, "iscrizione del apparato del "@it;
	rdfs:comment """Entry of text structure apparatus."""@en;
	rdfs:subClassOf textedit:ApparatusEntry, [
		a owl:Restriction; owl:onProperty textedit:isEntryOfStructureApparatus; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:isEntryOfStructureApparatusValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:entryHasStructureDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger].

textedit:Witness
	a owl:Class;
	rdfs:label "witness text"@en, "Textzeuge"@de;
	rdfs:comment """Copy of a text studied in a critical edition, as comparison or proof of existance of the original by looking for features of originality, possibly enabling the original's reconstruction."""@en;
	rdfs:subClassOf text:CopiedText, text:Expression.

textedit:Siglum
	a owl:Class;
	rdfs:label "siglum"@en, "Siglum"@de, "siglum"@fr, "siglum"@it;
	rdfs:comment """One or more symbols as a reference to a witness text."""@en;
	rdfs:subClassOf document:TextReference.

textedit:Reading
	a owl:Class;
	rdfs:label "reading"@en, "Textinterpretation"@de, "lezione"@it, "lectio"@la;
	rdfs:comment """Copy of one or more words or phrases of a text part of a witness."""@en;
	rdfs:subClassOf textedit:EditorialStructure, [
		a owl:Restriction; owl:onProperty textedit:hasWitness; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:hasWitnessValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].	

textedit:Lemma
	a owl:Class;
	rdfs:label "lemma"@en, "Lemma"@de, "lemma"@fr, "lemma"@it;
	rdfs:comment """One or more words or phrases in a critical apparatus referring to its edition text, and as appearing in a witness or created by an editor, and judged by the editor to be authentic, accurate, and/or authoritative, and compaired with variants referring to other witnesses"""@en;
	skos:note """In an apparatus entry a lemma is seperated from a variant or editorial explanation by a right square bracket ']'."""@en;
	rdfs:subClassOf textedit:EditorialStructure, [
		a owl:Restriction; owl:onProperty textedit:isLemmaOfEntry; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:isLemmaOfEntryValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:lemmaHasTextReference; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:lemmaHasTextReferenceValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:lemmaHasVariant; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:lemmaHasVariantValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

textedit:NewLemma # textedit:Emendation
	a owl:Class;
	rdfs:label "new lemma"@en, "neues Lemma"@de; #, "lemma"@it, "lemma"@la;
	rdfs:comment """Lemma created by an editor, not a reading from a witness."""@en;
	rdfs:subClassOf textedit:Lemma.

textedit:Variant
	a owl:Class;
	rdfs:label "variant"@en, "Variant"@de, "variante"@fr, "variante"@it;
	rdfs:comment """One or more words or phrases, or their absence, of a critical edition, judged by an editor to be unsatisfactory for linguistic and/or stylistic reasons, but of interest for establishing the text, and as such varying from what the editor has judged to be the lemma."""@en;
	rdfs:subClassOf textedit:EditorialStructure, textedit:Reading, [
		a owl:Restriction; owl:onProperty textedit:isVariantOfEntry; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:isVariantOfEntryValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:variantHasTextReference; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty textedit:variantHasTextReferenceValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

#
# PROPERTIES
#

textedit:hasEditor
	a owl:ObjectProperty;
	rdfs:label "edition has editor"@en, "Edition hat Editor"@de;
	rdfs:comment """Relating an edition to a person who created it."""@en;
	rdfs:domain textedit:Edition;
#	rdfs:range [rdfs:subClassOf human:Person, [a owl:Restriction; owl:onProperty human:personHasRole; owl:someValuesFrom textedit:Editor]];
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint textedit:Edition;
	knora-base:objectClassConstraint human:Person.

textedit:hasEditorValue
	a owl:ObjectProperty;
	rdfs:label "edition has editor - statement"@en, "Edition hat Editor - Festlegung"@de;
	rdfs:comment """Relating an edition to a reification statement of the relation between the edition and a person who created it."""@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint textedit:Edition;
	knora-base:objectClassConstraint knora-base:LinkValue.

textedit:hasRedactor
	a owl:ObjectProperty;
	rdfs:label "has redactor"@en, "hat Redaktor"@de;
	rdfs:comment """Relating a text to a person who redacted it."""@en;
	rdfs:domain text:Text;
#	rdfs:range [rdfs:subClassOf human:Person, [a owl:Restriction; owl:onProperty human:personHasRole; owl:someValuesFrom textedit:Redactor]];
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint human:Person.

textedit:hasRedactorValue
	a owl:ObjectProperty;
	rdfs:label "has redactor - statement"@en, "hat Redaktor - Festlegung"@de;
	rdfs:comment """Relating a text to a reification statement of the relation between the text and a person who redacted it."""@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint knora-base:LinkValue.

textedit:hasGlossator
	a owl:ObjectProperty;
	rdfs:label "has glossator"@en, "hat Glossator"@de;
	rdfs:comment """Relating a text to a person who glossed it."""@en;
	rdfs:domain text:Text;
#	rdfs:range [rdfs:subClassOf human:Person, [a owl:Restriction; owl:onProperty human:personHasRole; owl:someValuesFrom text:Glossator]];
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint human:Person.

textedit:hasGlossatorValue
	a owl:ObjectProperty;
	rdfs:label "has glossator - statement"@en, "hat Glossator - Festlegung"@de;
	rdfs:comment """Relating a text to a reification statement of the relation between the text and a person who glossed it."""@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint knora-base:LinkValue.

textedit:hasAbbreviator
	a owl:ObjectProperty;
	rdfs:label "has abbreviator"@en, "hat Abkurzer"@de;
	rdfs:comment """Relating a text to a person who abbreviated it."""@en;
	rdfs:domain text:Text;
#	rdfs:range [rdfs:subClassOf human:Person, [a owl:Restriction; owl:onProperty human:personHasRole; owl:someValuesFrom text:Abbreviator]];
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint human:Person.

textedit:hasAbbreviatorValue
	a owl:ObjectProperty;
	rdfs:label "has abbreviator - statement"@en, "hat Abkurzer - Festlegung"@de;
	rdfs:comment """Relating a text to a reification statement of the relation between the text and a person who abbreviated it."""@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint knora-base:LinkValue.

textedit:hasEdition
	a owl:ObjectProperty;
	rdfs:label "text has edition"@en, "Text hat Edition"@de;
	rdfs:comment "Relating a text to an edition it has."@en;
	rdfs:domain text:Text;
	rdfs:range textedit:Edition;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint textedit:Edition.

textedit:hasEditionValue
	a owl:ObjectProperty;
	rdfs:label "text has edition - statement"@en, "Text hat Edition - Festlegung"@de;
	rdfs:comment "Relating a text to a reification statement of the relation between the text and an edition thereof."@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint knora-base:LinkValue.

#:hasEditedContent
#	a owl:ObjectProperty;
#	rdfs:label "has edited content"@en;
#	rdfs:comment """Relating an edition to its content."""@en;
#	rdfs:subPropertyOf language:hasContent;
#	knora-base:subjectClassConstraint textedit:Edition;
#	knora-base:objectClassConstraint knora-base:TextValue.

textedit:hasSameEditionAs
	a owl:ObjectProperty;
	rdfs:label "has same edition as"@en, "hat gleiche Edition als"@de;
	rdfs:comment """Relating two texts with a same edition."""@en;
	rdfs:domain text:Text;
	rdfs:range text:Text;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint text:Text.

textedit:hasSameEditionAsValue
	a owl:ObjectProperty;
	rdfs:label "has same edition as - statement"@en, "hat gleiche Edition als - Festlegung"@de;
	rdfs:comment """Relating a text to a reification statement of the relation between the text and another one with a same edition."""@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint knora-base:LinkValue.

textedit:hasTransliteration
	a owl:ObjectProperty;
	rdfs:label "has transliteration"@en, "hat Transliteration"@de;
	rdfs:comment """Relating a text to a transliteration thereof."""@en;
	rdfs:domain text:Text;
	rdfs:range textedit:Transliteration;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint textedit:Transliteration.

textedit:hasTransliterationValue
	a owl:ObjectProperty;
	rdfs:label "has transliteration - statement"@en, "hat Transliteration - Festlegung"@de;
	rdfs:comment """Relating a text to a reification statement of the relation between the text and a transliteration thereof."""@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint knora-base:LinkValue.

textedit:hasDerivedTextExpression
	a owl:ObjectProperty;
	rdfs:label "has derived text expression"@en, "hat abgeleitete Text-Expression"@de;
	rdfs:comment "Relating a text expression to another one derived from it."@en;
	rdfs:domain text:Expression;
	rdfs:range textedit:DerivedTextExpression;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint text:Expression;
	knora-base:objectClassConstraint textedit:DerivedTextExpression.

textedit:hasDerivedTextExpressionValue
	a owl:ObjectProperty;
	rdfs:label "has derived text expression - statement"@en, "hat abgeleitete Text-Expression - Festlegung"@de;
	rdfs:comment "Relating a text expression to a reification statement of the relation between the text expression and another one derived from it."@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint text:Expression;
	knora-base:objectClassConstraint knora-base:LinkValue.

textedit:hasScientificEdition
	a owl:ObjectProperty;
	rdfs:label "has scientific text edition"@en, "hat wissenschaftliche Textedition"@de;
	rdfs:comment "Relating a text expression to a scientific edition thereof."@en;
	rdfs:domain text:Expression;
