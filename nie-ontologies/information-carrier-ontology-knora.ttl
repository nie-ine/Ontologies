@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix cidoc: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix frbroo: <http://iflastandards.info/ns/fr/frbr/frbroo/>.
@prefix physical: <http://www.knora.org/ontology/0055/physical-resource#>.
@prefix event: <http://www.knora.org/ontology/0047/event#>.
@prefix human: <http://www.knora.org/ontology/0048/human#>.
@prefix concept: <http://www.knora.org/ontology/0044/concept#>.
@prefix document: <http://www.knora.org/ontology/005C/document#>.
@prefix text: <http://www.knora.org/ontology/0059/text#>.
@prefix device: <http://www.knora.org/ontology/0045/device#>.
@prefix knora-base: <http://www.knora.org/ontology/knora-base#>.

@base <http://www.knora.org/ontology/004C/information-carrier>.
@prefix infocar: <http://www.knora.org/ontology/004C/information-carrier#>.

<http://www.knora.org/ontology/004C/information-carrier>
	a owl:Ontology;
	dct:title "An ontology about information carrier"@en;
	dct:description """Formal description of information carrier in broadest sense, general subclasses, related classes, and properties."""@en;
	dct:creator "Hans Cools, MD, knowledge engineer, ontologist, software developer, NIE-INE, Basel University, Switzerland"@en;
	dct:publisher "Basel University, Switzerland"@en;

	knora-base:attachedToProject <http://rdfh.ch/projects/004C>.

#
# CLASSES
#

infocar:Carrier
 	a owl:Class;
 	rdfs:label "information carrier"@en,"Informationsträger"@de, "support d'information"@fr;
 	rdfs:comment """Carrier of information created by a person."""@en;
	skos:note """Includes both F4_Manifestation_Singleton and F5_Item as declared in FRBROO."""@en;
	rdfs:subClassOf human:PhysicalCreation, cidoc:E84_Information_Carrier, [
		a owl:Restriction; owl:onProperty infocar:carrierHasDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:hasSender; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:hasSenderValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:hasReceiver; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:hasReceiverValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

infocar:Set
	a owl:Class;
	rdfs:label "information carrier set"@en, "Informationsträger-Reihe"@de, "ensemble des supports d'information"@fr;
	rdfs:comment """Information carriers belonging together, e.g. a series of volumes, an archive, , nachlass."""@en;
	rdfs:subClassOf human:PhysicalCreation, [
		a owl:Restriction; owl:onProperty infocar:hasCarrierDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger].

infocar:Copy
	a owl:Class;
	rdfs:label "information carrier copy"@en, "Informationsträgerkopie"@de;
	rdfs:comment """Carrier with a copy of information."""@en;
	rdfs:subClassOf human:Copy.

infocar:Leaf
	a owl:Class;
	rdfs:label "leaf"@en, "Blatt"@de;
	rdfs:comment """Sheet of a writable substance, e.g. paper, parchment, treated plant leaf."""@en;
	rdfs:subClassOf infocar:Carrier, [
		a owl:Restriction; owl:onProperty infocar:isLeafOfBook; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isLeafOfBookValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

# fold, Falte, cf. Gedicht Heinrich Heine

infocar:Page
	a owl:Class;
	rdfs:label "page"@en, "Seite"@de;
	rdfs:comment """One of the 2 surfaces of a leaf, e.g. a manuscript, or of a leaf as part of a bundled set of leafs, e.g. a book."""@en;
	rdfs:subClassOf infocar:Carrier, [
		a owl:Restriction; owl:onProperty infocar:isPageOfLeaf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isPageOfLeafValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isPageOfBook; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isPageOfBookValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:pageHasNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger].

infocar:Book
	a owl:Class;
	rdfs:label "book"@en, "Buch"@de;
	rdfs:comment """Set of leafs, e.g. sheets of paper or parchment, binded together along one side, with a front and back cover."""@en;
	rdfs:subClassOf infocar:Carrier.

infocar:Binding
	a owl:Class;
	rdfs:label "binding"@en, "Einband"@de;
	rdfs:comment """Material holding the leafs and covers of a book together."""@en;
	rdfs:subClassOf infocar:Carrier, [
		a owl:Restriction; owl:onProperty infocar:isBindingOfBook; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isBindingOfBookValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

infocar:Volume
	a owl:Class;
	rdfs:label "volume"@en, "Band"@de;
	rdfs:comment """One of the books carrying an expression materialized in more than one book."""@en;
	rdfs:subClassOf infocar:Book, [
		a owl:Restriction; owl:onProperty infocar:isVolumeOfWork; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isVolumeOfWorkValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [		
		a owl:Restriction; owl:onProperty infocar:volumeHasNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger].

infocar:PartitionedWork
	a owl:Class;
	rdfs:label "partitioned work"@en, "partitioniertes Werk"@de;
	rdfs:comment """Set of volumes carrying a common expression, e.g. a dictionary."""@en;
	rdfs:subClassOf infocar:Set.

infocar:Printing
	a owl:Class;
	rdfs:label "printing"@en, "Drucken"@de;
	rdfs:comment """Action of putting an expression on a carrier, e.g. with ink on paper."""@en;
	rdfs:subClassOf human:Procedure, frbroo:F32_Carrier_Production_Event.

infocar:AgentPrinter
	a owl:Class;
	rdfs:label "agent printer"@en, "Agent-Drucker"@de;
	rdfs:comment """Role of a human agent, person or organization, printing an expression."""@en;
	rdfs:subClassOf human:AgentRole.

infocar:PersonPrinter
	a owl:Class;
	rdfs:label "person printer"@en, "Person-Drucker"@de;
	rdfs:comment """Role of a person printing an expression."""@en;
	rdfs:subClassOf human:PersonCreator, infocar:AgentPrinter.

infocar:Print
	a owl:Class;
	rdfs:label "print"@en, "Druck"@de;
	rdfs:comment """An expression printed on a carrier."""@en;
	rdfs:subClassOf infocar:Carrier, [
		a owl:Restriction; owl:onProperty infocar:hasPrinter; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:hasPrinterValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isPartOfPrintRun; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isPartOfPrintRunValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

infocar:PrintRun
	a owl:Class;
	rdfs:label "print run"@en, "Druckauflage"@de, "Tirage"@fr, "Tiratura"@it;
	rdfs:comment """Collection of prints of the same expression, created at one time."""@en;
	skos:note """When applicable, all the prints have the same ISBN, but a different bar code."""@en;
	rdfs:subClassOf human:PhysicalCreation.

infocar:FirstPrint
	a owl:Class;
	rdfs:label "first print"@en, "Erstdruck"@de;
	rdfs:comment """Earliest print of an expression."""@en;
	rdfs:subClassOf infocar:Print.

infocar:Archive
	a owl:Class;
	rdfs:label "archive"@en, "Archiv"@de, "archive"@fr, "archivio"@it;
	rdfs:comment """Set of information carriers ordered along a method."""@en;
	rdfs:subClassOf infocar:Set, cidoc:E78_Collection.

infocar:ArchiveElement
	a owl:Class;
	rdfs:label "archive element"@en, "Archiv-Element"@de;
	rdfs:comment """Element of an archive, which can be a single file or a file group."""@en;
	rdfs:subClassOf infocar:Carrier, [
		a owl:Restriction; owl:onProperty infocar:isPartOfArchive; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isPartOfArchiveValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:hasArchiveSignature; owl:minCardinality "0"^^xsd:nonNegativeInteger].

infocar:Nachlass
	a owl:Class;
	rdfs:label "nachlass"@en, "Nachlass"@de;
	rdfs:comment """Set of information carriers as materialized expressions of a person as existing after the person's death and unpublished during life."""@en;
	rdfs:subClassOf infocar:Set, cidoc:E78_Collection.

infocar:NachlassElement
	a owl:Class;
	rdfs:label "nachlass element"@en, "Nachlass-Element"@de;
	rdfs:comment """Element of a nachlass."""@en;
	rdfs:subClassOf infocar:Carrier, [
		a owl:Restriction; owl:onProperty infocar:isPartOfNachlass; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isPartOfNachlassValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

infocar:WritingUtensil
	a owl:Class;
	rdfs:label "writing utensil"@en, "Schreibzeug"@de;
	rdfs:comment """Object to write with."""@en;
	rdfs:subClassOf human:PhysicalCreation.

infocar:Pencil
	a owl:Class;
	rdfs:label "pencil"@en, "Bleistift"@de;
	rdfs:comment """Elongated cylindrical(-like) object containing a thin rod of graphite for writing or drawing."""@en;
	rdfs:subClassOf infocar:WritingUtensil.

infocar:Pen
	a owl:Class;
	rdfs:label "pen"@en, "Tintenstift"@de;
	rdfs:comment """Elongated cylindrical(-like) hollow object containing ink for writing or drawing."""@en;
	rdfs:subClassOf infocar:WritingUtensil.

infocar:FountainPen
	a owl:Class;
	rdfs:label "fountain pen"@en, "Füller"@de;
	rdfs:comment """Pen with refillable reservoir for fluid ink for feeding a metal nib."""@en;
	rdfs:subClassOf infocar:Pen.

infocar:Typewriter
	a owl:Class;
	rdfs:label "typewriter"@en, "Schreibmaschine"@de;
	rdfs:comment """Writing device with moving parts for letter printing."""@en; # mechanical or electronic 
	rdfs:subClassOf infocar:WritingUtensil.

infocar:Manuscript
	a owl:Class;
	rdfs:label "manuscript"@en, "Manuskript"@de;
	rdfs:comment """Carrier with a handwritten text."""@en;
	rdfs:subClassOf infocar:Carrier, frbroo:F4_Manifestation_Singleton.

infocar:PalmLeafManuscript
	a owl:Class;
	rdfs:label "palm-leaf manuscript"@en, "Palmblattmanuskript"@de;
	rdfs:comment """Palm leaf manuscript."""@en;
	skos:Note """Dried and smoke treated palm leaves were used as writing carrier in the Indian subcontinent and Southeast since at least the 5th century BC, possibly much earlier."""@en;
	rdfs:subClassOf infocar:Leaf, infocar:Manuscript.

infocar:NoteCarrier
	a owl:Class;
	rdfs:label "note carrier"@en,"Notizträger"@de;
	rdfs:comment """Carrier whereon notes are written, e.g. a first registration of thoughts."""@en;
	rdfs:subClassOf infocar:Carrier, frbroo:F4_Manifestation_Singleton.

infocar:Notebook
	a owl:Class;
	rdfs:label "notebook"@en,"Notizbuch"@de;
	rdfs:comment """Book wherein notes are written."""@en;
	rdfs:subClassOf infocar:Book, infocar:NoteCarrier.

infocar:Typescript
	a owl:Class;
	rdfs:label "typescript"@en, "Typoskript"@de;
	rdfs:comment """Carrier with a typewritten text."""@en;
	rdfs:subClassOf infocar:Carrier, frbroo:F4_Manifestation_Singleton.

infocar:Transcript
	a owl:Class;
	rdfs:label "transcript"@en, "Abschrift"@de;
	rdfs:comment """Carrier with a text copied by character, by any method, e.g. handwriting, typewriting, printing, digitizing."""@en;
	rdfs:subClassOf human:Copy, infocar:Carrier, frbroo:F4_Manifestation_Singleton, [
		a owl:Restriction; owl:onProperty infocar:hasTranscriber; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:hasTranscriberValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

infocar:TextCopy
	a owl:Class;
	rdfs:label "text copy"@en, "Textkopie"@de;
	rdfs:comment """Carrier with a copy of a text."""@en;
	rdfs:subClassOf infocar:Copy.

# homonym: transcript: conversion from different manifestation, eg. from spoken or recorded sound to written

# facsimile

infocar:ManualTranscript
	a owl:Class;
	rdfs:label "manual transcript"@en, "manuelle Abschrift"@de;
	rdfs:comment """Carrier with a handwritten copy of a text."""@en;
	rdfs:subClassOf infocar:Transcript, infocar:Manuscript.

infocar:Diary #°
	a owl:Class;
	rdfs:label "diary"@en,"Tagebuch"@de;
	rdfs:comment """information carrier with periodically, usually daily, described personal experiences and observations."""@en;
	rdfs:subClassOf infocar:Carrier.

infocar:PersonSender
	a owl:Class;
	rdfs:label "person sender"@en, "Person-Sender"@de;
	rdfs:comment """Role of a person sending information on a carrier."""@en;
	rdfs:subClassOf human:PersonSender.

infocar:PersonReceiver
	a owl:Class;
	rdfs:label "person receiver"@en, "Person-Empfänger"@de;
	rdfs:comment """Role of a person receiving information on carrier."""@en;
	rdfs:subClassOf human:PersonReceiver.

infocar:Letter
	a owl:Class;
	rdfs:label "letter"@en, "Brief"@de, "lettre"@fr, "lettera"@it;
	rdfs:comment """Written text, e.g. on a leaf, intended to be sent from (a) person(s) or (an) Organization(s) to another one, e.g. through a postal system."""@en;
	rdfs:subClassOf infocar:Carrier, frbroo:F4_Manifestation_Singleton.

infocar:Postcard
	a owl:Class;
	rdfs:label "postcard"@en, "Postkarte"@de, "carte postale"@fr, "cartolina"@it;
	rdfs:comment """Card with one side, usually with a printed structure, used for writing a message, an address and applying a postage stamp, to be sent through a postal system."""@en;
	rdfs:subClassOf infocar:Carrier, frbroo:F4_Manifestation_Singleton.

infocar:Convolute #° version
	a owl:Class;
	rdfs:label "information carrier convolute"@en, "Informationsträger-Konvolut"@de;
	rdfs:comment """Set of information carriers, e.g. handwritten drafts, manuscripts, typoscripts or prints, with a commonality, e.g. being of the same author."""@en;
	rdfs:subClassOf infocar:Set, [
		a owl:Restriction; owl:onProperty infocar:convoluteHasTitle; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:convoluteHasDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:convoluteHasSizeDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:convoluteHasOriginDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:convoluteHasContentRepresentation; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:containsEarlierStagesOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:containsEarlierStagesOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:containsEarlierStagesOfPublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:containsEarlierStagesOfPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:containsLaterStagesOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:containsLaterStagesOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

infocar:ConvoluteElement
	a owl:Class;
	rdfs:label "convolute element"@en, "Konvolut-Element"@de;
	rdfs:comment """Element of an information carrier convolute."""@en;
	rdfs:subClassOf infocar:Carrier, [
		a owl:Restriction; owl:onProperty infocar:isPartOfConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isPartOfConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

infocar:ElectronicCarrier #° data or information storage device
	a owl:Class;
	rdfs:label "electronic information carrier"@en,"elektronischer Informationsträger"@de;
	rdfs:comment """Electronic machine carrying electronic information."""@en;
	rdfs:subClassOf device:ElectronicMachine, infocar:Carrier.

infocar:ElectronicBook
	a owl:Class;
	rdfs:label "electronic book"@en,"elektronisches Buch"@de;
	rdfs:comment """Electronic information carrier with a text structure as in a book."""@en;
	rdfs:subClassOf infocar:ElectronicCarrier.

infocar:WebPage
	a owl:Class;
	rdfs:label "web page"@en, "Webseite"@de;
	rdfs:comment """Electronic carrier page on the World Wide Web."""@en;
	skos:note """Not necessarily published, i.e. made public."""@en;
	rdfs:subClassOf infocar:ElectronicCarrier, [
		a owl:Restriction; owl:onProperty infocar:webPageHasName; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:webPageHasURL; owl:minCardinality "0"^^xsd:nonNegativeInteger].

infocar:Hyperlink # software
	a owl:Class;
	rdfs:label "hyperlink"@en,"Hyperlink"@de;
	rdfs:comment """Link from a hypertext document structure to another location, activated by clicking on a highlighted word or image."""@en;
	rdfs:subClassOf knora-base:Resource, [
		a owl:Restriction; owl:onProperty infocar:hyperlinkHasName; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:hyperlinkHasURL; owl:minCardinality "0"^^xsd:nonNegativeInteger].

#
# PROPERTIES
#

infocar:carrierHasDescription
	a owl:ObjectProperty;
	rdfs:label "has description"@en, "hat Beschreibung"@de;
	rdfs:comment """Relating an information carrier to a description thereof."""@en;
	rdfs:subPropertyOf text:hasDescription;
	knora-base:subjectClassConstraint infocar:Carrier;
	knora-base:objectClassConstraint knora-base:TextValue.

infocar:isOnCarrier
	a owl:ObjectProperty;
	rdfs:label "is on carrier"@en, "ist auf Träger"@de;
	rdfs:comment "Relating information to a carrier it is on."@en;
	rdfs:domain concept:Information;
	rdfs:range infocar:Carrier;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint concept:Information;
	knora-base:objectClassConstraint infocar:Carrier.

infocar:isOnCarrierValue
	a owl:ObjectProperty;
	rdfs:label "is on carrier - statement"@en, "ist auf Träger - Festlegung"@de;
	rdfs:comment "Relating information to a reification statement of the relation between the information and a carrier it is on."@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint concept:Information;
	knora-base:objectClassConstraint knora-base:LinkValue.

infocar:isOnElectronicCarrier
	a owl:ObjectProperty;
	rdfs:label "is on electronic carrier"@en, "ist auf elektronischen Träger"@de;
	rdfs:comment "Relating information to an electronic carrier it is on."@en;
	rdfs:domain concept:Information;
	rdfs:range infocar:ElectronicCarrier;
	rdfs:subPropertyOf infocar:isOnCarrier;
	knora-base:subjectClassConstraint concept:Information;
	knora-base:objectClassConstraint infocar:ElectronicCarrier.

infocar:isOnElectronicCarrierValue
	a owl:ObjectProperty;
	rdfs:label "is on electronic carrier - statement"@en, "ist auf elektronischen Träger - Festlegung"@de;
	rdfs:comment "Relating information to a reification statement of the relation between the information and an electronic carrier it is on."@en;
	rdfs:subPropertyOf infocar:isOnCarrierValue;
	knora-base:subjectClassConstraint concept:Information;
	knora-base:objectClassConstraint knora-base:LinkValue.

infocar:isPageOnCarrier
	a owl:ObjectProperty;
	rdfs:label "is page on carrier"@en, "ist Seite auf Träger"@de;
	rdfs:comment """Relating a document page to a carrier page it is on."""@en;
	rdfs:domain document:Page;
	rdfs:range infocar:Page;
	rdfs:subPropertyOf infocar:isOnCarrier;
	knora-base:subjectClassConstraint document:Page;
	knora-base:objectClassConstraint infocar:Page.

infocar:isPageOnCarrierValue
	a owl:ObjectProperty;
	rdfs:label "is page on carrier - statement"@en, "ist Seite auf Träger - Festlegung"@de;
	rdfs:comment """Relating a document page to a reification statement of the relation between the page and a carrier page it is on."""@en;
	rdfs:subPropertyOf infocar:isOnCarrierValue;
	knora-base:subjectClassConstraint document:Page;
	knora-base:objectClassConstraint knora-base:LinkValue.

infocar:isTextOnPage
	a owl:ObjectProperty;
	rdfs:label "is text on page"@en, "ist Text auf Seite"@de;
	rdfs:comment """Relating a text to a carrier page it is on."""@en;
	rdfs:domain text:Text;
	rdfs:range infocar:Page;
	rdfs:subPropertyOf infocar:isOnCarrier;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint infocar:Page.

infocar:isTextOnPageValue
	a owl:ObjectProperty;
	rdfs:label "is text on page - statement"@en, "ist Text auf Seite - Festlegung"@de;
	rdfs:comment """Relating a text to a reification statement of the relation between the text and a carrier page it is on."""@en;
	rdfs:subPropertyOf infocar:isOnCarrierValue;
	knora-base:subjectClassConstraint text:Text;
	knora-base:objectClassConstraint knora-base:LinkValue.

infocar:isWrittenWith
	a owl:ObjectProperty;
	rdfs:label "is written with"@en, "ist geschrieben mit"@de;
	rdfs:comment "Relating a text to a utensil it is written with."@en;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint text:WrittenText;
	knora-base:objectClassConstraint infocar:WritingUtensil.

infocar:isWrittenWithValue
	a owl:ObjectProperty;
	rdfs:label "is is written with - statement"@en, "ist geschrieben mit - Festlegung"@de;
	rdfs:comment "Relating a text to a reification statement of the relation between the text and a utensil it is written with."@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint text:WrittenText;
	knora-base:objectClassConstraint knora-base:LinkValue.

infocar:hasCarrierDescription
	a owl:ObjectProperty;
	rdfs:label "has information carrier description"@en, "hat Textträger-Beschreibung"@de;
	rdfs:comment """Relating an information carrier set to a description of an information carrier it contains."""@en;
	rdfs:domain infocar:Set;
	rdfs:range rdfs:Literal;
	rdfs:subPropertyOf text:hasDescription;
	knora-base:subjectClassConstraint infocar:Set;
	knora-base:objectClassConstraint knora-base:TextValue.

infocar:isPageOfLeaf
	a owl:ObjectProperty;
