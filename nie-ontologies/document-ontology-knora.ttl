document:isElementOfDocument
	a owl:ObjectProperty;
	rdfs:label "is element of document"@en, "ist Element eines Dokumentes"@de;
	rdfs:comment """Relating a document element to a document it is part of."""@en;
	rdfs:domain document:Element;
	rdfs:range document:Document;
	rdfs:subPropertyOf concept:isPartOf;
	knora-base:subjectClassConstraint document:Element;
	knora-base:objectClassConstraint document:Document.

document:isElementOfDocumentValue
	a owl:ObjectProperty;
	rdfs:label "is element of document - statement"@en, "ist Element eines Dokumentes - Festlegung"@de;
	rdfs:comment """Relating a document element to a reification statement of the relation between the element and a document it is part of."""@en;
	rdfs:subPropertyOf concept:isPartOfValue;
	knora-base:subjectClassConstraint document:Element;
	knora-base:objectClassConstraint knora-base:LinkValue.

document:hasIdentifier
	a owl:ObjectProperty;
	rdfs:label "document content structure has identifier"@en, "Dokumentinhaltstruktur hat Identifikator"@de;
	rdfs:comment """Relating a document content structure to its identifier."""@en;
	rdfs:domain document:ContentStructure;
	rdfs:range document:ContentStructureIdentifier;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint document:ContentStructure;
	knora-base:objectClassConstraint document:ContentStructureIdentifier.

document:hasIdentifierValue
	a owl:ObjectProperty;
	rdfs:label "document content structure has identifier - statement"@en, "Dokumentinhaltstruktur hat Identifikator - Festlegung"@de;
	rdfs:comment """Relating a document content structure to a reification statement of the relation between the content structure and its identifier."""@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint document:ContentStructure;
	knora-base:objectClassConstraint knora-base:LinkValue.

document:hasReference
	a owl:ObjectProperty;
	rdfs:label "document content structure has reference"@en, "Dokumentinhaltstruktur hat Referenz"@de;
	rdfs:comment """Relating a document content structure to a reference it has."""@en;
	rdfs:domain document:ContentStructure;
	rdfs:range document:ContentStructureReference;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint document:ContentStructure;
	knora-base:objectClassConstraint document:ContentStructureReference.

document:hasReferenceValue
	a owl:ObjectProperty;
	rdfs:label "document content structure has reference - statement"@en, "Dokumentinhaltstruktur hat Referenz - Festlegung"@de;
	rdfs:comment """Relating a document content structure to a reification statement of the relation between the content structure and a reference it has."""@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint document:ContentStructure;
	knora-base:objectClassConstraint knora-base:LinkValue.

document:hasTextReference
	a owl:ObjectProperty;
	rdfs:label "has text reference"@en, "hat Textreferenz"@de;
	rdfs:comment """Relating a document content structure to a text reference it has."""@en;
	rdfs:domain document:ContentStructure;
	rdfs:range document:TextReference;
	rdfs:subPropertyOf document:hasReference;
	knora-base:subjectClassConstraint document:ContentStructure;
	knora-base:objectClassConstraint document:TextReference.

document:hasTextReferenceValue
	a owl:ObjectProperty;
	rdfs:label "has text reference - statement"@en, "hat Textreferenz - Festlegung"@de;
	rdfs:comment """Relating a document content structure to a reification statement of the relation between the content structure and a text reference it has."""@en;
	rdfs:subPropertyOf document:hasReferenceValue;
	knora-base:subjectClassConstraint document:ContentStructure;
	knora-base:objectClassConstraint knora-base:LinkValue.

document:hasFootnoteReference
	a owl:ObjectProperty;
	rdfs:label "has footnote reference"@en, "hat Fussnotereferenz"@de;
	rdfs:comment """Relating a document content structure to a footnote reference it has."""@en;
	rdfs:domain document:ContentStructure;
	rdfs:range document:FootnoteReference;
	rdfs:subPropertyOf document:hasTextReference;
	knora-base:subjectClassConstraint document:ContentStructure;
	knora-base:objectClassConstraint document:FootnoteReference.

document:hasFootnoteReferenceValue
	a owl:ObjectProperty;
	rdfs:label "has footnote reference - statement"@en, "hat Fussnotereferenz - Festlegung"@de;
	rdfs:comment """Relating a document content structure to a reification statement of the relation between the content structure and a footnote reference it has."""@en;
	rdfs:subPropertyOf document:hasTextReferenceValue;
	knora-base:subjectClassConstraint document:ContentStructure;
	knora-base:objectClassConstraint knora-base:LinkValue.

document:hasEndnoteReference
